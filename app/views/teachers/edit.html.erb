<h1>Edit <%= @teacher.name%> </h1>

<%= form_for @teacher do |f| %>
<%= f.label :name, "Name:" %>
<%= f.text_field :name %>
<%= f.label :age, "Age:" %>
<%= f.number_field :age %>
<%= f.collection_select :school_id, @schools, :id, :name, prompt: "Select a School"%>
<%= f.submit "Submit" %>

<% end %>